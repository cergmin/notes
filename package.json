{
  "name": "obs",
  "version": "0.0.1",
  "description": "Obs Notes",
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "author": "Sergey Minakov <cergmin@gmail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fix": "pnpm fix:prettier & pnpm fix:eslint & pnpm fix:stylelint",
    "fix:prettier": "prettier --write \"(src|public)/**/*.(js|jsx|ts|tsx|css)\"",
    "fix:eslint": "eslint --fix --ext js,jsx,ts,tsx src",
    "fix:stylelint": "stylelint --fix \"(src|public)/**/*.css\""
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@mavrin/remark-typograf": "^2.2.0",
    "clsx": "^1.1.1",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rehype-katex": "^6.0.2",
    "rehype-slug": "^5.0.1",
    "remark-math": "^5.1.1",
    "remark-parse": "^10.0.1",
    "typograf": "^6.14.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.13.0",
    "eslint-plugin-react": "^7.30.1",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.3"
  }
